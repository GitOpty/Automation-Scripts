{
	"name": "EXECL_BLOB_DIN",
	"properties": {
		"linkedServiceName": {
			"referenceName": "blob_din",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileSystem": {
				"type": "string"
			},
			"File": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Excel",
		"typeProperties": {
			"sheetName": {
				"value": "@concat('Base ',toUpper(replace(dataset().File,'fpa','')))",
				"type": "Expression"
			},
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().File",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().FileSystem",
					"type": "Expression"
				}
			},
			"firstRowAsHeader": true
		},
		"schema": []
	}
}