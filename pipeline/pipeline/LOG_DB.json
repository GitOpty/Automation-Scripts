{
	"name": "LOG_DB",
	"properties": {
		"activities": [
			{
				"name": "LOG_SADALLA",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "OracleSource",
						"oracleReaderQuery": "\nSELECT \nB.USER_ID\n,C.USERNAME\n,A.SQL_FULLTEXT\n,A.LAST_LOAD_TIME\n,A.COMMAND_TYPE\nFROM V$SQL A\nINNER JOIN DBA_HIST_ACTIVE_SESS_HISTORY B ON B.SQL_ID = A.SQL_ID\nINNER JOIN DBA_USERS C ON C.USER_ID = B.USER_ID\nWHERE A.COMMAND_TYPE <> 3",
						"partitionOption": "None",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Sadalla",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Az_Sadalla",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Teste"
		},
		"annotations": []
	}
}