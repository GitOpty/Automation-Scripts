				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "LIMPA TABELAS DESTINO",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('BUSCA_TABELA').output.value",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copiar Dados DW",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
