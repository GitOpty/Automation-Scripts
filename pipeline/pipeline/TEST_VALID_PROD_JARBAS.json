{
	"name": "TEST_VALID_PROD_JARBAS",
	"properties": {
		"activities": [
			{
				"name": "DW PROD VALID",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT DISTINCT A.*\nFROM DW.TBL_PROD_DIARIA A WITH(NOLOCK)\nWHERE YEAR(A.DT_ENTRADA) >= 2021 ",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "TRUNCATE TABLE DW.tbl_prod_diaria_valid",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "AZ_SQL_JARBAS",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AZ_PROD_VALID",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Producao_Diaria"
		},
		"annotations": []
	}
}