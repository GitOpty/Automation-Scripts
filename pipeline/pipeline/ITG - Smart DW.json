{
	"name": "ITG - Smart DW",
	"properties": {
		"activities": [
			{
				"name": "Table to ETL",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT DISTINCT \nCAT.COD_TABELA\n,CAT.NOM_SCHEMA_ORIGEM + '.' + CAT.NOM_TABELA_ORIGEM    AS TB_ORIGEM\n,CAT.NOM_SCHEMA_DESTINO + '.' + CAT.NOM_TABELA_DESTINO  AS TB_DESTINO\n,CAT.NOM_SCHEMA_DESTINO\n,CAT.NOM_TABELA_DESTINO\n,LA.DSC_PARAMETROS\n,CAT.IND_ETL\nFROM DBO.CTR_ADF_TABELA CAT (NOLOCK)\nINNER JOIN DBO.LOG_ADF LA (NOLOCK) ON LA.COD_TABELA = CAT.COD_TABELA\nWHERE LA.COD_SITUACAO = 0\nAND CAT.COD_TABELA IN (10,14)",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "AZ_ETL_DESTINO",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "ArrayTableETL",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@variables('ARRAY_TABELAS')",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "TbOrigem",
							"type": "SetVariable",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"variableName": "ORIGEM_TB",
								"value": {
									"value": "@item().TB_ORIGEM",
									"type": "Expression"
								}
							}
						},
						{
							"name": "TbDestino",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "TbOrigem",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"variableName": "DESTINO_TB",
								"value": {
									"value": "@item().TB_DESTINO",
									"type": "Expression"
								}
							}
						},
						{
							"name": "Query",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "TbDestino",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"variableName": "QUERY",
								"value": {
									"value": "@item().DSC_PARAMETROS",
									"type": "Expression"
								}
							}
						},
						{
							"name": "SchemaDestino",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "Query",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"variableName": "SCHEMA_DEST",
								"value": {
									"value": "@item().NOM_SCHEMA_DESTINO",
									"type": "Expression"
								}
							}
						},
						{
							"name": "NmTbDestino",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "SchemaDestino",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"variableName": "TABLE_DEST",
								"value": {
									"value": "@item().NOM_TABELA_DESTINO",
									"type": "Expression"
								}
							}
						},
						{
							"name": "TpETL",
							"description": "U = Update, Truncate table\nI = Incremental",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "NmTbDestino",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"variableName": "TP_ETL",
								"value": {
									"value": "@item().IND_ETL",
									"type": "Expression"
								}
							}
						},
						{
							"name": "Switch1",
							"type": "Switch",
							"dependsOn": [
								{
									"activity": "TpETL",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"on": {
									"value": "@variables('TP_ETL')",
									"type": "Expression"
								},
								"cases": [
									{
										"value": "U",
										"activities": [
											{
												"name": "Update Table DW",
												"type": "Copy",
												"dependsOn": [],
												"policy": {
													"timeout": "7.00:00:00",
													"retry": 0,
													"retryIntervalInSeconds": 30,
													"secureOutput": false,
													"secureInput": false
												},
												"userProperties": [],
												"typeProperties": {
													"source": {
														"type": "SqlServerSource",
														"sqlReaderQuery": {
															"value": "@variables('QUERY')",
															"type": "Expression"
														},
														"queryTimeout": "02:00:00",
														"partitionOption": "None"
													},
													"sink": {
														"type": "AzureSqlSink",
														"preCopyScript": {
															"value": "@{concat('TRUNCATE TABLE  ', variables('DESTINO_TB'))}",
															"type": "Expression"
														},
														"disableMetricsCollection": false
													},
													"enableStaging": false,
													"translator": {
														"type": "TabularTranslator",
														"typeConversion": true,
														"typeConversionSettings": {
															"allowDataTruncation": true,
															"treatBooleanAsNumber": false
														}
													}
												},
												"inputs": [
													{
														"referenceName": "SQL_ORIG",
														"type": "DatasetReference"
													}
												],
												"outputs": [
													{
														"referenceName": "AZ_ETL_DESTINO",
														"type": "DatasetReference"
													}
												]
											}
										]
									}
								]
							}
						}
					]
				}
			},
			{
				"name": "ArrayTableETL",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Table to ETL",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "ARRAY_TABELAS",
					"value": {
						"value": "@activity('Table to ETL').output.value",
						"type": "Expression"
					}
				}
			}
		],
		"variables": {
			"ARRAY_TABELAS": {
				"type": "Array"
			},
			"ORIGEM_TB": {
				"type": "String"
			},
			"DESTINO_TB": {
				"type": "String"
			},
			"QUERY": {
				"type": "String"
			},
			"SCHEMA_DEST": {
				"type": "String"
			},
			"TABLE_DEST": {
				"type": "String"
			},
			"TP_ETL": {
				"type": "String"
			}
		},
		"folder": {
			"name": "DW Ciclo/Try Old"
		},
		"annotations": []
	}
}