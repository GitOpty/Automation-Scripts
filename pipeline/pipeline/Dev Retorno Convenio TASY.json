									"type": "Double",
									"physicalType": "float"
								}
							},
							{
								"source": {
									"name": "VLR_PAGO",
									"type": "Double"
								},
								"sink": {
									"name": "VLR_PAGO",
									"type": "Double",
									"physicalType": "float"
								}
							},
							{
								"source": {
									"name": "NR_SEQUENCIA",
									"type": "Decimal"
								},
								"sink": {
									"name": "NR_SEQUENCIA",
									"type": "Decimal",
									"physicalType": "decimal",
									"scale": 0,
									"precision": 18
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Oracle_Update_ETL",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AZ_AUX_RET_CONV",
						"type": "DatasetReference"
					}
				]
			}
		],
