				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "OracleSource",
						"oracleReaderQuery": "SELECT\n    TMP.*\nFROM(\n    SELECT        \n        CRM.NR_SEQUENCIA\n        ,CR.NR_SEQUENCIA        AS NR_SEQ_RETORNO\n        ,CRM.NR_CONTA\n        ,CRM.NR_SEQ_ITEM_CONTA\n        ,0                  AS NR_TITULO\n        ,CRM.CD_ITEM\n        ,CR.DT_RECEBIMENTO\n        ,CRM.DT_ATUALIZACAO\n        ,CRM.VL_COBRADO     AS VLR_GUIA\n        ,CRM.VL_GLOSA       as VLR_GLOSA\n        ,CRM.VL_PAGO        AS VLR_PAGO\n        ,0                  AS VLR_AMENOR\n        ,ROW_NUMBER() OVER (PARTITION BY CRM.CD_ITEM,CRM.NR_CONTA,CRM.NR_SEQ_ITEM_CONTA ORDER BY CRM.DT_ATUALIZACAO DESC) ID_ROW\n    FROM CONVENIO_RETORNO_MOVTO CRM\n    INNER JOIN CONVENIO_RETORNO CR ON CR.NR_SEQUENCIA = CRM.NR_SEQ_RETORNO\n    LEFT JOIN CONVENIO_RETORNO_ITEM CRI ON CRI.NR_SEQUENCIA = NR_SEQ_RET_ITEM\n    --WHERE TRUNC(CRM.DT_ATUALIZACAO,'MM') > TRUNC(SYSDATE,'MM') - 180\n)TMP\nWHERE TMP.ID_ROW = 1",
						"partitionOption": "None",
						"queryTimeout": "02:00:00"
					},
