								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": true,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "ORACLE_LOG",
									"type": "DatasetReference",
									"parameters": {
										"nm_keyvault": {
											"value": "@item().NOME_KEYVAULT",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DESTINO_TABELAS_ADF",
